package kr.co.shovvel.dm.domain.management.login;

import lombok.ToString;

import com.thoughtworks.xstream.annotations.XStreamAlias;

import kr.co.shovvel.dm.domain.common.PrevSessionUser;

@XStreamAlias( "loginSessionUser" )
@ToString
public class LoginDomain extends PrevSessionUser {

	private int		retryCount;
	private int		cnt;			/* SO_USER_SESSION.CNT 접속카운트 */

	private String	sessionId;
