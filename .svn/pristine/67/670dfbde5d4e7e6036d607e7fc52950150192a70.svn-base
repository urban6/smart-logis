/*loading*/
.loading.type_01 { border: 3px solid #dcdce0; border-top: 3px solid #f9413c; border-radius: 50%; width: 20px; height: 20px; margin: 0 auto; animation: ani_01 2s linear infinite; }
.loading.type_02 { margin: 0  auto; display: block; width: 24px; height: 24px; position: relative; border: 4px solid #f9413c; animation: ani_02 3.5s infinite ease; overflow: hidden; }
.loading.type_02 i { vertical-align: top; display: inline-block; width: 100%; background-color: #f9413c; animation: ani_03 3.5s infinite ease-in; }
.loading.type_04, .loading.type_03 { position: relative; width: 70px; height: 70px; margin: 0 auto; animation: ani_04 8s linear infinite; }
.loading.type_04 i, .loading.type_03 i { position: absolute; left: 50%; top: 40px; width: 50px; height: 50px; }
.loading.type_04 i:after, .loading.type_03 i:after { content: ''; position: absolute; width: 50px; height: 50px; border-radius: 100%; border-left: 6px solid #6677cc; border-bottom: 1px solid #5b6bba; animation: ani_05 1s linear infinite; }
.loading.type_04 i:nth-child(1), .loading.type_03 i:nth-child(1) { transform: rotate(10deg); }
.loading.type_04 i:nth-child(2), .loading.type_03 i:nth-child(2) { transform: rotate(105deg); }
.loading.type_04 i:after { border-left: 6px solid #d10a0a; border-bottom: 1px solid #b90000; }

