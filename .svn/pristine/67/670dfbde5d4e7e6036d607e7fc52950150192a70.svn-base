.switch span:after { content: ""; display: block; position: absolute; top: 2px; left: 2px; width: 26px; height: 26px; border-radius: 100px; -webkit-border-radius: 100px; background-color: #fff; box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2) !important; -webkit-box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2); -moz-box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2); transition: all 0.3s; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; }
