}

$body_bg: #fafafa;
$color_01: #00b3ff; /*active border color*/
$color_02: #ed2424; /*error border color*/

/* btn set */
$btn_shadow_inset: inset 0px 3px 5px rgba(0,0,0,0.3);










@mixin box_style_01 {
  -webkit-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 0 0 #b2b4b9;
  box-shadow: 0 1px 0 0 #b2b4b9;
}

@mixin box_style_02 {
  -webkit-border-radius: 5px;
  border-radius: 5px;
  -webkit-box-shadow: 0 1px 0 0 #b2b4b9;
  box-shadow: 0 1px 0 0 #b2b4b9;
}

@mixin box_shadow_01 {
  -webkit-box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.25);
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.25);
}






