@charset "UTF-8";

@mixin font_type_01 {
  // font-family: 'LatoWeb', 'Malgun Gothic', sans-serif;
  font-family: 'segoe ui', 'Malgun Gothic', sans-serif;
  // html[class^='ie'] & {
  //   font-family: 'Malgun Gothic', sans-serif;
